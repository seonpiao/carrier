.gift
  display:block
  height:144px;
  .gift_con_listL
    width:468px
    height:109px
    background-color:#0c1924
    border:1px solid #1774a4
    padding-top:40px
    position:relative
    .tips
      position:absolute
      top:5px
      right:25px
      img
        vertical-align:middle
      .num
        color:#f9f03a
      .helpicon
        width:11px;
        height:14px;
        background:url($bgUrl+'player3/sgift_icon.png') no-repeat 0 -108px;
        position:absolute;
        right:-16px;
        top:1px;
      .helpicon:hover
        background:url($bgUrl+'player3/sgift_icon.png') no-repeat -14px -108px;
    .viewport
      width:430px
      overflow:hidden
      height:100%
      position:absolute
      margin-top:-10px
      overflow:hidden;
      left:18px;
      .overview
        position:absolute
      .overview.gift_show_list li
        width:76px;
        margin:0 5px;
        cursor:pointer;
        display:inline;
        .c_markbg
          width:39px;
          height:39px;
          background:url($bgUrl+'player3/sgift_icon.png') no-repeat 0 0;
          position:absolute;
          left:5px;
          top:-1px;
          text-align:center;
          ins
            display:block;
            position:absolute;
            left:50%;
            top:2px;
            margin-left:-16px;
          .cmark_1
            width:21px;
            height:21px;
            background:url($bgUrl+'player3/sgift_icon.png') no-repeat 0 -40px;
          .cmark_2
            width:18px;
            height:18px;
            background:url($bgUrl+'player3/sgift_icon.png') no-repeat 0 -62px;
          .cmark_3
            width:26px;
            height:26px;
            top:0;
            background:url($bgUrl+'player3/sgift_icon.png') no-repeat 0 -82px;
          .cmark_4
            width:25px;
            height:25px;
            top:0;
            background:url($bgUrl+'player3/sgift_icon.png') no-repeat 0 -154px;
      .gift_show_list li.has_child:hover
        .c_markbg
          width:29px
          height:29px
          background:url($bgUrl + 'player3/sgift_icon.png') no-repeat 0 -123px;
          left:7px
          top:4px
          ins
            margin-left:-14px;
            top:-3px;
    .gift_leftbtn.disable,.gift_leftbtn.prev
      position:absolute;
      left:8px
      top:40px;
    .gift_rightbtn.disable,.gift_rightbtn.next
      position:absolute;
      right:8px;
      top:40px;
.gift_sigel_list
  height:74px;
  border: 1px solid #0c1218;
  background-color: #0c1924;
  border-bottom-color:#11212e;
  padding:3px 5px;
  text-align:center;
  margin:0 auto;
  position:absolute
  -moz-border-radius:3px;      /* Gecko browsers */
  -webkit-border-radius:3px;   /* Webkit browsers */
  border-radius:3px;            /* W3C syntax */
  left:-5px
  z-index:20
  .gift_arrow
    width:12px;
    height:10px;
    background:url($bgUrl+'player3/gamegiftarrow_icon.png') no-repeat center bottom;
    position:absolute;
    left:30px;
    bottom:-10px;
  .giftsigel_item
    margin-top:3px;
    .mt_2
      margin-top:2px;
.usercrittips
