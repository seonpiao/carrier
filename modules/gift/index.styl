.gift
  display:block
  height:144px;
.gift_sigel_list
  height:75px;
  border: 1px solid #0c1218;
  background-color: #0c1924;
  padding:5px;
  text-align:center;
  margin:0 auto;
  position:absolute
  -moz-border-radius:2px;      /* Gecko browsers */
  -webkit-border-radius:2px;   /* Webkit browsers */
