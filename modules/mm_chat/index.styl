#chat_window textarea
  margin: 0 auto
  height: 100px
.chat_window
  margin: 0 auto
  padding: 4px
  border: none
  overflow-y: auto
.chat_names
  text-align: right
  color: #ED145B
  word-wrap: break-word
  vertical-align: top
  font-weight: bold
  width: 70px;
.chat_content
  text-align: left
  word-wrap: break-word
  width: 100%
.chat_box
  text-align: left
  word-wrap: break-word
  word-break: break-all
  height:184px
  padding-top:8px
  overflow: hidden
  width: 100%
  line-height:20px
  font-family:"微软雅黑",Arial

.chat_box em
  font-style:normal
  color:#c7c2b6
  margin-right:4px
  font-family:Arial
  font-size:10px
  -webkit-transform:scale(0.84,0.84) 
  *font-size:10px
  display:inline-table
.chat_wiew .uname
  cursor:pointer
.chat_box p.fromNotice
  color:#fff
.chat_box p.noticeRed
  color:#ff4023
.chat_box p.grayText2
  color:#cdc7c3
.chat_text
  position:relative
  z-index:1
.chatText,input.chatText
  border:#1774a4 solid 1px
  border-radius:2px
  height:12px
  line-height:12px
  color:#fff
  display:inline-block
  position:relative
  z-index:1
  padding:5px 2px
  width:206px
  border:#1774a4 solid 1px
  background:transparent
  font-size:12px
  font-family:"微软雅黑"
  font-weight:normal
  margin-left:2px
input.chatText:hover,input.chatText:focus
  background:transparent
  color:#fff
.btn_chat
  display: inline-block
  height: 24px
  width: 48px
  float: left
  margin-left: 0px
  background: #2ec2e5
  text-align: center
  line-height: 24px
  font-size:12px
  font-family:"微软雅黑"
  border-radius:4px
  cursor:pointer
  margin-left:4px
a.btn_chat:hover
  background:#43d1f4
  color:#fff
a.btn_chat:active
  background:#037aa2
  color:#fff
.chat_holder
  display:inline-block
  height:24px
  line-height:24px
  width:202spx
  padding:0 6px
  position:absolute
  z-index:1
  left:0px
  top:0px                                                     
  position:absolute
  z-index:2
  display:inline-block
  color:#808080

.chat_box .sending
  background:url($bgUrl+'sending.gif') no-repeat left center
  background-size: 12px 12px
.chat_box .sent
  background:none;
.emoticon
  display:block
  text-indent:-9999px
  width:26px
  height:26px
  background:url($bgUrl+'emobtn.png') no-repeat -2px -2px
.emoticon:hover,.emoticon.hover
  background:url($bgUrl+'emobtn.png') no-repeat -2px -30px
  //margin-top:2px
#emoticonDropDown 
  position:absolute
  z-index:100
  padding:0
  display:none
  left:0px
  height:160px
  width:256px
.emotionDropDownInner .scrollbar
  top:5px
.emotionDropDownInner .emoticonsArea
  margin:0
  background:#273851
  border:#1774a4 solid 1px
  padding:5px 5px 5px 5px
.emotionsDropDownInner .viewport
