/*自定义样式*/
.merchants_rightmain
	width:549px;
	height:310px;
	padding:10px 7px;
	border:1px solid #1774a4;
	background-color:#273851;
	border-radius:3px;
	-webkit-box-shadow:1px 0px 2px #2f4662 inset,0 1px 2px #2f4662 inset,0 -1px 2px #2f4662 inset,-1px 0 2px #2f4662 inset;
	-moz-box-shadow:1px 0px 2px #2f4662 inset,0 1px 2px #2f4662 inset,0 -1px 2px #2f4662 inset,-1px 0 2px #2f4662 inset;
	box-shadow:1px 0px 2px #2f4662 inset,0 1px 2px #2f4662 inset,0 -1px 2px #2f4662 inset,-1px 0 2px #2f4662 inset;
.merchants_rightmain
	position:relative;
	margin-left:113px;
	z-index:1;
	font-family: "微软雅黑","microsoft yahei",Arial;
	color:#fff;
	font-size:12px;
	margin-top:-5px;
.merchantsGames .ui-dialog-grid,.bottleGames .ui-dialog-grid
	width: 100%;
.merchantsGames .ui-dialog-body,.bottleGames .ui-dialog-body
	padding:0;
.merchantsGames .ui-dialog-title,.bottleGames .ui-dialog-title
	padding:0;
	position:relative;
	overflow:visible;
.merchantsGames .ui-dialog-header,.bottleGames .ui-dialog-header
	border:none;
.merchantsGames .ui-dialog-close
	width:15px;
	height:15px;
	background:url($bgUrl+'prop_icon/bottolbg.png') no-repeat -585px 0;
	right:-5px;
	opacity:1;
	top:-5px;
	z-index:1040;
.merchantsGames .ui-dialog-close:hover,.merchantsGames .ui-dialog-close:focus
	background:url($bgUrl+'prop_icon/bottolbg.png') no-repeat -585px -15px;
	opacity:1;
.bottleGames .ui-dialog-close
	width:15px;
	height:15px;
	background:url($bgUrl+'prop_icon/bottolbg.png') no-repeat -585px 0;
	right:-13px;
	opacity:1;
	top:-5px;
	z-index:1040;
.bottleGames .ui-dialog-close:hover,.bottleGames .ui-dialog-close:focus
	background:url($bgUrl+'prop_icon/bottolbg.png') no-repeat -585px -15px;
	opacity:1;
.dialogBluebgGames .ui-dialog-close
	right:1px;
	top:-5px;
.persianMerchants_img
	width:223px;
	height:357px;
	position:absolute;
	left:-45px;
	top:-19px;
.persianMerchants_title span
	width:211px;
	height:35px;
	background:url($bgUrl+'prop_icon/prop_iconbg.png') no-repeat 0 -260px;
	position:absolute;
	left:170px;
	top:-41px;
.ui-popup-focus .ui-dialog.merchantsGames
	padding:10px;
	background:url($bgUrl+'bg_white.png') repeat 0 0;
.merchants_rightcon ul li
	width:220px;
	height:80px;
	border-radius:4px;
	background-color:#0c1924;
	padding:5px;
	float:left;
.onceflush_btn
	width:86px;
	height:107px;
	background:url($bgUrl+'prop_icon/oncerefresh_link.png') no-repeat 0 0;
.onceflush_btn:hover
	background:url($bgUrl+'prop_icon/oncerefresh_hover.png') no-repeat 0 -2px;
.once_flushcon
	position:absolute;
	left:231px;
	top:95px;
	z-index:1;
.once_flushcon p
	margin-left:-15px;
	margin-top: -2px;
