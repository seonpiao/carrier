.ladycamprank
  .goddess_ranking_main
    font-family "微软雅黑","microsoft yahei",Arial
    color #fff
    font-size 12px
    border 1px solid #0c1218
    border-radius 2px
    background-color #0c1924
    box-shadow 1px 0px 2px #172a39 inset,0 1px 2px #172a39 inset,0 -1px 2px #172a39 inset,-1px 0 2px #172a39 inset
    -webkit-box-shadow 1px 0px 2px #172a39 inset,0 1px 2px #172a39 inset,0 -1px 2px #172a39 inset,-1px 0 2px #172a39 inset
    -moz-box-shadow 1px 0px 2px #172a39 inset,0 1px 2px #172a39 inset,0 -1px 2px #172a39 inset,-1px 0 2px #172a39 inset
    margin-left 115px
    margin-top 30px
    z-index 1
    position relative

  .goddess_ranking_con
    width 359px
    height 395px
    padding-top 5px
    padding-right 5px
    overflow hidden

  .goddess_ranking_tab
    width 368px
    height 30px
    position absolute
    top -30px
    left -3px
    li
      width 88px
      height 28px
      line-height 28px
      color #fff
      text-align center
      border 1px solid #0c1218
      float left
      margin-left 2px
      background-color #062e4f
      border-radius 2px
      border-bottom none
      cursor pointer
      &.on
        background-color #0c1924
        margin-bottom -2px
        border-bottom-color #0c1924
        height 31px

  .goddess_rankinggirls
    height 453px
    position absolute
    left -73px
    top -10px

  .mt_020
    margin-top -20px

  .w18
    width 15px
    display inline-block
    text-align center
    vertical-align middle
  .w24
    width 20px
    display inline-block
    text-align center
    vertical-align middle

  .goddess_ranking_offer
    height 356px
    margin-bottom 10px
    .scrollbars
      width:350px
      height:335px;
      .viewport,.overview
        width:350px
        height:335px;
      .scrollbar
        right:16px;
  .popularity_offer_pk
    width 350px
    height 155px
    li
      width 140px
      height 104px
      text-align center
      margin 0 auto
      position absolute
      letter-spacing -1px
      &.user_photo_1
        left 50%
        margin-left -72px
        top -23px
      &.user_photo_2
        left 0
        bottom 25px
        .pk_user_photpbg2
          img
            top 44px
      &.user_photo_3
        right 0
        bottom 25px
        .pk_user_photpbg3
          img
            top 44px
      img
        height 44px
        background url($bgUrl + "player3/purple_photobg.png") no-repeat 0 0
        padding 3px 2px 2px
        position absolute
        top 40px
        right 32px
      div.photo_downC
        height:13px;
        vertical-align: middle
        line-height:13px;
        img
          vertical-align: middle
          margin-left: 3px
          max-height: 12px
          position:static;
  .pk_user_photpbg1,.pk_user_photpbg2,.pk_user_photpbg3
    position relative
    background url($bgUrl + "player3/ranking_photobg.png") no-repeat
    margin 0 auto
    z-index 1

  .pk_user_photpbg1
    width 110px
    height 115px
    background-position 0 0
    ins
      width 26px
      height 14px
      background url($bgUrl + "player3/ranking_img.png") no-repeat -234px -56px
      position absolute
      bottom 18px
      left 40px

  .pk_user_photpbg2
    width 110px
    height 116px
    background-position 0 -118px
    ins
      width 27px
      height 15px
      background url($bgUrl + "player3/ranking_img.png") no-repeat -234px -78px
      position absolute
      bottom 18px
      left 42px

  .pk_user_photpbg3
    width 110px
    height 116px
    background-position 0 -255px
    ins
      width 27px
      height 14px
      background url($bgUrl + "player3/ranking_img.png") no-repeat -234px -101px
      position absolute
      bottom 18px
      left 42px

  .angel_icon_lingts
    width 14px
    height 15px
    background url($bgUrl + "player3/yellow_1.png") no-repeat 0 0
    display inline-block
    vertical-align middle

  .angel_icon_gray
    width 14px
    height 15px
    background url($bgUrl + "player3/gray_2.png") no-repeat 0 0
    display inline-block
    vertical-align middle

  .vip_icon
    width 18px
    height 8px
    background url($bgUrl + "player3/vipbtn.png") no-repeat 0 0
    display inline-block
    vertical-align middle

  .ranking_footerbg
    width 331px
    height 26px
    line-height 26px
    background url($bgUrl + "player3/ranking_wordbg.png") no-repeat 0 0
    padding 0 23px 0 10px

  .ranking_hr
    width 275px
    height 1px
    margin 5px auto 8px
    background url($bgUrl + "player3/ranking_hr.png") no-repeat 0 0

  .popularity_offer_rank
    width:340px;
    padding 0 5px 5px 5px
    li
      margin-bottom 4px
      vertical-align middle
      width 340px
      float left
      display inline
      img
        vertical-align: middle
        margin-left: 3px
        max-height: 16px
      div
        float:left;
        height:15px;
        vertical-align: middle
        overflow:hidden;
      div.qtrank
        width:40px;
        text-align:center;
      div.u_namerank
        margin-left:10px
      div.popurlarlist
        float:right;
        text-align:left;
      div.populargx
        width:74px;
        float:left;
        text-align:left;
      div.popularrank
       float:left;
       width:90px;
       text-align:left;
    li.populargx_title
      div
        height:16px;
        line-height:16px;
        font-size:12px;
        color:#fff;
        vertical-align:middle;
        text-align:center;
        background url($bgUrl + "prop_icon/rank_a_hr.png") no-repeat right center;
      div.popularrank
        background:none;
  .dialogGames
    .ui-dialog-close
      position absolute
      right 5px
      opacity 1
      top 3px

  .goddess_ranking_title
    width 262px
    height 50px
    background url($bgUrl + 'player3/ranking_img.png') no-repeat 0 0
    position absolute
    left 55px
    top -82px
    span
      width 155px
      height 23px
      background url($bgUrl + 'player3/ranking_img.png') no-repeat 0 -56px
      display block
      margin 10px 0 0 50px

  .mr_25
    margin-right 25px

  .goddess_ranking_list
    width 30px
    height 102px
    position absolute
    right -30px
    top -1px
    li
      width 30px
      height 50px
      color #fff
      border 1px solid #0c1218
      line-height 25px
      text-align center
      background-color #062e4f
      border-radius 2px
      border-bottom none
      cursor pointer
      margin-bottom 2px
      &.on
        background-color #0c1924
        margin-left -2px
        border-left-color #0c1924
        width 31px
        border-radius 2px

  .goddess_ranking_list li:hover,.goddess_ranking_tab li:hover
    background-color #153f64

  .goddess_ranking_list li.on:hover,.goddess_ranking_tab li.on:hover
    background-color #0c1924

